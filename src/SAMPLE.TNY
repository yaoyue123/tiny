{ Sample program
  in TINY language
}
char x;
char letterp; char letterl; char lettere;
int number;
letterp := 112; letterl := 108; lettere := 101;
write 12*3;
read x;
read number;
repeat
  write number;
  write x;
  if x = 97
  then
    write letterp;
    write letterp;
    write letterl;
    write lettere
  else
    if x = 65
    then
      write letterp;
      write lettere
    else
      write x+1
    end
  end;
  write 10000*1000;
  number := number + 1;
  x := x + 1
until 300 < number